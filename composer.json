{
    "name": "ttek/tk2base",
    "type": "project",
    "description": "A Tk3 base MVC framework project",
    "keywords":  [
        "tropotek"
    ],
    "time": "2019-04-09",
    "license": "MIT",
    "version": "3.0.0",
    "minimum-stability": "dev",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "Tk\\Composer\\SetupEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Tk\\Composer\\SetupEvent::postUpdate"
        ]
    },
    "extra":  {
        "branch-alias":  {
            "dev-master": "3.0.x-dev"
        }
    },
    "require":  {
        "php": ">=7.0",
        "ttek/tk-auth": "dev-3beta as 3.0.x-dev",
        "ttek/tk-base": "dev-3beta as 3.0.x-dev",
        "ttek/tk-database": "dev-3beta as 3.0.x-dev",
        "ttek/tk-domtemplate": "dev-3beta as 3.0.x-dev",
        "ttek/tk-form": "dev-3beta as 3.0.x-dev",
        "ttek/tk-framework": "dev-3beta as 3.0.x-dev",
        "ttek/tk-http": "dev-3beta as 3.0.x-dev",
        "ttek/tk-installers": "dev-3beta as 3.0.x-dev",
        "ttek/tk-mail": "dev-3beta as 3.0.x-dev",
        "ttek/tk-mvc": "dev-3beta as 3.0.x-dev",
        "ttek/tk-plugin": "dev-3beta as 3.0.x-dev",
        "ttek/tk-routing": "dev-3beta as 3.0.x-dev",
        "ttek/tk-table": "dev-3beta as 3.0.x-dev",
        "ttek/tk-ui": "dev-3beta as 3.0.x-dev",

        "ttek/theme-cube": "~1.0",

        "sensiolabs/ansi-to-html": "~1.0",
        "lasserafn/php-initial-avatar-generator": "dev-master",

        "symfony/http-foundation": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/console": "^4.2",
        "symfony/translation": "^4.2"
    },
    "require-dev":  {
    }
}
